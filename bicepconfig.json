{
    "analyzers": {
      "core": {
        "enabled": true,
        "verbose": false,
        "rules": {
          "adminusername-should-not-be-literal": {
            "level": "warning"
          },
          "no-hardcoded-env-urls": {
            "level": "off"
          },
          "no-unnecessary-dependson": {
            "level": "warning"
          },
          "no-unused-params": {
            "level": "warning"
          },
          "no-unused-vars": {
            "level": "warning"
          },
          "outputs-should-not-contain-secrets": {
            "level": "warning"
          },
          "prefer-interpolation": {
            "level": "warning"
          },
          "secure-parameter-default": {
            "level": "warning"
          },
          "simplify-interpolation": {
            "level": "warning"
          },
          "use-protectedsettings-for-commandtoexecute-secrets": {
            "level": "warning"
          },
          "use-stable-vm-image": {
            "level": "warning"
          }
        }
      }
    }
  }